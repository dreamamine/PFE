{% extends "LGMAdministrationBundle::layout.html.twig" %}
{% block page_body %}
    <h3> Voulez vous vraiment supprimer le code : <strong style="color:red;">
            {{theme.denomination}}</strong> de votre theme </h3>
<form method="POST" action="{{path('delete_theme', {'token': theme.token} )}}">
 <input type="submit" class="btn btn-danger" value="Oui" />   
<a href="{{path("index_theme")}}" class="btn btn-warning">Non</a>
{{form_rest(form)}}
</form>
        {% endblock %}